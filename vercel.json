{
    "version": 2,
    "builds": [
        {
            "src": "api/index.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/index.py"
        }
    ],
    "env": {
        "APP_NAME": "X Thread Analyzer",
        "DEBUG": "False",
        "PYTHONPATH": ".",
        "PORT": "8000"
    }
} 